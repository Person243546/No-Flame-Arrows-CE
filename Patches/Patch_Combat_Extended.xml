<?xml version="1.0" encoding="utf-8" ?>
<Patch>


	<Operation Class="PatchOperationRemove">
		<xpath>/Defs/CombatExtended.AmmoSetDef[defName="AmmoSet_Arrow"]</xpath>
	</Operation>
	
	<Operation Class="PatchOperationRemove">
		<xpath>/Defs/CombatExtended.AmmoSetDef[defName="AmmoSet_StreamlinedArrow"]</xpath>
	</Operation>
	
	<Operation Class="PatchOperationRemove">
		<xpath>/Defs/CombatExtended.AmmoSetDef[defName="AmmoSet_GreatArrow"]</xpath>
	</Operation>
	
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs</xpath>
		<value>
			
  <CombatExtended.AmmoSetDef>
    <defName>AmmoSet_Arrow</defName>
    <label>arrows</label>
    <ammoTypes>
      <Ammo_Arrow_Stone>Projectile_Arrow_Stone</Ammo_Arrow_Stone>
      <Ammo_Arrow_Steel>Projectile_Arrow_Steel</Ammo_Arrow_Steel>
      <Ammo_Arrow_Plasteel>Projectile_Arrow_Plasteel</Ammo_Arrow_Plasteel>
	    <Ammo_Arrow_Venom>Projectile_Arrow_Venom</Ammo_Arrow_Venom>
    </ammoTypes>
  </CombatExtended.AmmoSetDef>

  <CombatExtended.AmmoSetDef>
    <defName>AmmoSet_StreamlinedArrow</defName>
    <label>arrows</label>
    <ammoTypes>
      <Ammo_Arrow_Stone>Projectile_StreamlinedArrow_Stone</Ammo_Arrow_Stone>
      <Ammo_Arrow_Steel>Projectile_StreamlinedArrow_Steel</Ammo_Arrow_Steel>
      <Ammo_Arrow_Plasteel>Projectile_StreamlinedArrow_Plasteel</Ammo_Arrow_Plasteel>
	    <Ammo_Arrow_Venom>Projectile_StreamlinedArrow_Venom</Ammo_Arrow_Venom>
    </ammoTypes>
  </CombatExtended.AmmoSetDef>
  
   <CombatExtended.AmmoSetDef>
    <defName>AmmoSet_GreatArrow</defName>
    <label>great arrows</label>
    <ammoTypes>
      <Ammo_GreatArrow_Stone>Projectile_GreatArrow_Stone</Ammo_GreatArrow_Stone>
      <Ammo_GreatArrow_Steel>Projectile_GreatArrow_Steel</Ammo_GreatArrow_Steel>
      <Ammo_GreatArrow_Plasteel>Projectile_GreatArrow_Plasteel</Ammo_GreatArrow_Plasteel>
	  <Ammo_GreatArrow_Venom>Projectile_GreatArrow_Venom</Ammo_GreatArrow_Venom>
    </ammoTypes>
  </CombatExtended.AmmoSetDef>
			
		</value>
	</Operation>


	<Operation Class="PatchOperationRemove">
		<xpath>/Defs/ThingDef[defName="Ammo_GreatArrow_Flame"]</xpath>
	</Operation>

	<Operation Class="PatchOperationRemove">
		<xpath>/Defs/ThingDef[defName="Ammo_Arrow_Flame"]</xpath>
	</Operation>

	<Operation Class="PatchOperationRemove">
		<xpath>/Defs/ThingDef[defName="Projectile_Arrow_Flame"]</xpath>
	</Operation>

	<Operation Class="PatchOperationRemove">
		<xpath>/Defs/ThingDef[defName="Projectile_StreamlinedArrow_Flame"]</xpath>
	</Operation>
	
	<Operation Class="PatchOperationRemove">
		<xpath>/Defs/ThingDef[defName="Projectile_GreatArrow_Flame"]</xpath>
	</Operation>
	
	<Operation Class="PatchOperationRemove">
		<xpath>/Defs/RecipeDef[defName="MakeAmmo_Arrow_Flame"]</xpath>
	</Operation>
	
	<Operation Class="PatchOperationRemove">
		<xpath>/Defs/RecipeDef[defName="MakeAmmo_GreatArrow_Flame"]</xpath>
	</Operation>




</Patch>